{
    "name": "site-pro-monorepo",
    "description": "Vue3 Admin Pro",
    "scripts": {
        "clean": "rimraf -rf packages/*/{node_modules,es,lib,types} && rimraf node_modules",
        "build": "pnpm --filter=./packages/* run build",
        "publish": "pnpm --filter=./packages/* publish --no-git-checks",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^7.0.1",
        "ant-design-vue": "^4.2.3",
        "dayjs": "^1.11.12",
        "lodash-es": "^4.17.21",
        "vue": "^3.4.36",
        "vue-router": "^4.4.3"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.1.0",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vitejs/plugin-vue-jsx": "^4.1.0",
        "@vitest/coverage-v8": "^2.0.5",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.27.0",
        "jsdom": "^24.1.1",
        "rimraf": "^6.0.1",
        "typescript": "^5.5.4",
        "vite": "^5.4.6",
        "vite-plugin-dts": "^4.0.3",
        "vitest": "^2.0.5",
        "vue-tsc": "^2.0.29"
    },
    "peerDependencies": {
        "vue": ">=3.3.0",
        "ant-design-vue": ">=4.2.0",
        "@ant-design/icons-vue": ">=7.0.0"
    },
    "engines": {
        "node": "^18.0.0 || >=20.0.0"
    },
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    }
}

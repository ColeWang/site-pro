{
    "name": "site-pro-monorepo",
    "version": "1.0.0-alpha.1",
    "description": "Vue3 Admin Pro",
    "type": "module",
    "scripts": {
        "clean": "rimraf -rf packages/*/{es,lib,types}",
        "clean:deps": "rimraf -rf packages/*/{node_modules} && rimraf node_modules",
        "reset": "pnpm run clean && pnpm run clean:deps && pnpm install",
        "build": "pnpm --filter=./packages/* run build",
        "build:publish": "pnpm --filter=./packages/* publish --no-git-checks",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "test": "vitest --config ./vitest.config.ts",
        "test:coverage": "vitest run --config ./vitest.config.ts --coverage"
    },
    "dependencies": {
        "@ant-design/icons-vue": "^7.0.1",
        "ant-design-vue": "^4.2.6",
        "dayjs": "^1.11.13",
        "lodash-es": "^4.17.21",
        "vue": "^3.5.18"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^5.2.0",
        "@types/jsdom": "^21.1.7",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^24.2.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vitejs/plugin-vue-jsx": "^5.0.1",
        "@vitest/coverage-v8": "^3.2.4",
        "@vue/test-utils": "^2.4.6",
        "axios": "^1.11.0",
        "axios-mock-adapter": "^2.1.0",
        "eslint": "^9.32.0",
        "glob": "^11.0.3",
        "jsdom": "^26.1.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.2",
        "vite": "^7.0.6",
        "vite-plugin-dts": "^4.5.4",
        "vitepress": "^1.6.4",
        "vitest": "3.2.4",
        "vue-router": "^4.5.1",
        "vue-tsc": "^3.0.5"
    },
    "engines": {
        "node": "^20.19.0 || ^22.12.0"
    },
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    }
}

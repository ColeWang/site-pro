{
    "name": "@site-pro/locale",
    "version": "0.3.1",
    "description": "Vue3 Admin Pro Locale of Vue",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build"
    },
    "types": "./types/index.d.ts",
    "module": "./es/index.js",
    "main": "./lib/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./es/index.js",
            "require": "./lib/index.js"
        },
        "./*": {
            "types": "./types/*.d.ts",
            "import": "./es/*.js",
            "require": "./lib/*.js"
        }
    },
    "files": [
        "lib",
        "es",
        "types",
        "package.json"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}

{
    "name": "@site-pro/locale",
    "version": "0.4.5",
    "description": "Vue3 AdminPro Locale of Vue",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "TIMING=1 eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "types": "./types/index.d.ts",
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./es/index.js",
            "types": "./types/index.d.ts"
        },
        "./en-US": {
            "require": "./lib/en-US.js",
            "import": "./es/en-US.js",
            "types": "./types/en-US.d.ts"
        },
        "./zh-CN": {
            "require": "./lib/zh-CN.js",
            "import": "./es/zh-CN.js",
            "types": "./types/zh-CN.d.ts"
        }
    },
    "files": [
        "lib",
        "es",
        "types",
        "README.md",
        "package.json"
    ],
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}

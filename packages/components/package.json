{
    "name": "@site-pro/components",
    "version": "0.4.5",
    "description": "Vue3 AdminPro Components of Vue",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "TIMING=1 eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@site-pro/hooks": "workspace:*",
        "@site-pro/locale": "workspace:*",
        "@site-pro/utils": "workspace:*"
    },
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "types": "./types/index.d.ts",
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./es/index.js",
            "types": "./types/index.d.ts"
        },
        "./action": {
            "require": "./lib/action/index.js",
            "import": "./es/action/index.js",
            "types": "./types/action/index.d.ts"
        },
        "./base-field": {
            "require": "./lib/base-field/index.js",
            "import": "./es/base-field/index.js",
            "types": "./types/base-field/index.d.ts"
        },
        "./base-form": {
            "require": "./lib/base-form/index.js",
            "import": "./es/base-form/index.js",
            "types": "./types/base-form/index.d.ts"
        },
        "./descriptions": {
            "require": "./lib/descriptions/index.js",
            "import": "./es/descriptions/index.js",
            "types": "./types/descriptions/index.d.ts"
        },
        "./editable-table": {
            "require": "./lib/editable-table/index.js",
            "import": "./es/editable-table/index.js",
            "types": "./types/editable-table/index.d.ts"
        },
        "./float-form": {
            "require": "./lib/float-form/index.js",
            "import": "./es/float-form/index.js",
            "types": "./types/float-form/index.d.ts"
        },
        "./form": {
            "require": "./lib/form/index.js",
            "import": "./es/form/index.js",
            "types": "./types/form/index.d.ts"
        },
        "./locale": {
            "require": "./lib/locale/index.js",
            "import": "./es/locale/index.js",
            "types": "./types/locale/index.d.ts"
        },
        "./query-filter": {
            "require": "./lib/query-filter/index.js",
            "import": "./es/query-filter/index.js",
            "types": "./types/query-filter/index.d.ts"
        },
        "./resize-observer": {
            "require": "./lib/resize-observer/index.js",
            "import": "./es/resize-observer/index.js",
            "types": "./types/resize-observer/index.d.ts"
        },
        "./table": {
            "require": "./lib/table/index.js",
            "import": "./es/table/index.js",
            "types": "./types/table/index.d.ts"
        },
        "./theme": {
            "require": "./lib/theme/index.js",
            "import": "./es/theme/index.js",
            "types": "./types/theme/index.d.ts"
        },
        "./ant-typings": {
            "require": "./lib/ant-typings.js",
            "import": "./es/ant-typings.js",
            "types": "./types/ant-typings.d.ts"
        }
    },
    "files": [
        "lib",
        "es",
        "types",
        "README.md",
        "package.json"
    ],
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}

{
  "name": "@site-pro/components",
  "version": "0.3.0",
  "description": "",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build"
  },
  "dependencies": {
    "@site-pro/hooks": "workspace:*",
    "@site-pro/locale": "workspace:*",
    "@site-pro/utils": "workspace:*"
  },
  "types": "./types/index.d.ts",
  "module": "./es/index.js",
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./*": {
      "types": "./types/*.d.ts",
      "import": "./es/*.js",
      "require": "./lib/*.js"
    }
  },
  "files": [
    "lib",
    "es",
    "package.json"
  ]
}

{
    "name": "@site-pro/components",
    "version": "0.3.1",
    "description": "Vue3 Admin Pro Components of Vue",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build"
    },
    "dependencies": {
        "@site-pro/hooks": "workspace:*",
        "@site-pro/locale": "workspace:*",
        "@site-pro/utils": "workspace:*"
    },
    "types": "./types/index.d.ts",
    "module": "./es/index.js",
    "main": "./lib/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./es/index.js",
            "require": "./lib/index.js"
        },
        "./*": {
            "types": "./types/*.d.ts",
            "import": "./es/*.js",
            "require": "./lib/*.js"
        }
    },
    "files": [
        "lib",
        "es",
        "types",
        "package.json"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}

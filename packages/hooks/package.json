{
    "name": "@site-pro/hooks",
    "version": "1.0.0-alpha.1",
    "description": "Vue3 AdminPro Hooks of Vue",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "typecheck": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "typecheck:vite": "vue-tsc --noEmit -p tsconfig.vite.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts"
    },
    "types": "./types/index.d.ts",
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "require": "./lib/index.js",
            "import": "./es/index.js"
        }
    },
    "dependencies": {
        "@site-pro/utils": "workspace:*"
    },
    "peerDependencies": {
        "ant-design-vue": ">=3.2.0",
        "lodash-es": ">=4.17.15",
        "vue": ">=3.2.0"
    },
    "files": [
        "lib",
        "es",
        "types",
        "README.md",
        "package.json"
    ],
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}

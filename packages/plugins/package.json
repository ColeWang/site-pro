{
    "name": "@site-pro/plugins",
    "version": "0.4.1",
    "description": "Vue3 Admin Pro Plugins of Vue",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "TIMING=1 eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@site-pro/hooks": "workspace:*",
        "@site-pro/utils": "workspace:*"
    },
    "types": "./types/index.d.ts",
    "module": "./es/index.js",
    "main": "./lib/index.js",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "import": "./es/index.js",
            "require": "./lib/index.js"
        },
        "./*": {
            "types": "./types/*.d.ts",
            "import": "./es/*.js",
            "require": "./lib/*.js"
        }
    },
    "files": [
        "lib",
        "es",
        "types",
        "README.md",
        "package.json"
    ],
    "keywords": [
        "vue",
        "site-pro",
        "admin",
        "admin-pro",
        "antd",
        "vue-antd",
        "ant-design-vue"
    ],
    "license": "MIT",
    "author": "ColeWang (https://github.com/ColeWang)",
    "repository": {
        "type": "git",
        "url": "https://github.com/ColeWang/site-pro"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}
